{
  "name": "n8n-nodes-contentstudio",
  "version": "2.0.5",
  "description": "n8n node for ContentStudio API (X-API-Key)",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "workflow",
    "contentstudio",
    "social",
    "api",
    "automation"
  ],
  "license": "MIT",
  "author": "D4 Interactive",
  "homepage": "https://github.com/d4interactive/n8n-nodes#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/d4interactive/n8n-nodes.git"
  },
  "bugs": {
    "url": "https://github.com/d4interactive/n8n-nodes/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "icons",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "nodes": [
      "dist/nodes/ContentStudio/ContentStudio.node.js"
    ],
    "credentials": [
      "dist/credentials/ContentStudioApi.credentials.js"
    ]
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "watch": "tsc -p tsconfig.json -w",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "echo 'Linting not configured'",
    "test": "echo 'Tests not configured'"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.34",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0",
    "n8n-core": ">=1.0.0"
  }
}
